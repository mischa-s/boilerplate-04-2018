    "fela": "^6.1.7",
    "final-form": "^4.5.2",
    "g": "^2.0.1",
    "helmet": "^3.12.0",
    "internal-nav-helper": "^1.0.2",
    "knex": "^0.14.4",
    "lodash": "^4.17.5",
    "material-ui": "^1.0.0-beta.43",
    "nodemailer": "^4.6.4",
    "path": "^0.12.7",

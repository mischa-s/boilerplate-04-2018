{
  "name": "boilerplate-04-2018",
  "description": "a boilerplate, maybe",
  "version": "0.0.0",
  "keywords": [
    "feathers",
    "boilerplate"
  ],
  "author": {
    "name": "Mischa Saunders",
    "email": "mischa.saunders@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": "^8.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "start": "npm run server",
    "server": "QT_QPA_PLATFORM='' node --max-old-space-size=480 ./API",
    "build": "node -optimize_for_size --max_old_space_size=800 --gc_interval=100 ./node_modules/.bin/webpack --progress -p",
    "heroku-postbuild": "npm run build",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint ./API/. test/. --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --exit",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "QT_QPA_PLATFORM='' node-dev ./API",
    "dev:bundler": "webpack-dev-server --inline --mode development"
  },
